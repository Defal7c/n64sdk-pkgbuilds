pkgname=n64sdk
pkgver=2.0L
pkgrel=1
pkgdesc="N64 SDK libraries and include files"
url="https://github.com/CrashOveride95/n64sdkmod"
arch=('any')
options=('!strip')
depends=('n64sdk-common')
conflicts=('iquesdk')
license=('custom')
source=("git+https://github.com/CrashOveride95/n64sdkmod.git")
sha256sums=('SKIP')

package() {
  cp -dr --no-preserve='ownership,mode' "${srcdir}/n64sdkmod/packages/n64sdk/usr" "${pkgdir}"
}
